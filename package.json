{
  "name": "r3f-next-starter",
  "version": "1.2.6",
  "authors": [
    "Renaud ROHLINGER <hello@renaudrohlinger.com>"
  ],
  "private": true,
  "scripts": {
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint --fix 'src/**/*.{js,jsx}' --ext jsconfig.json",
    "prettier": "prettier --list-different '**/*.{js,jsx,md}'",
    "dev": "next dev",
    "build": "node postinstall.js && next build",
    "start": "cross-env NODE_ENV=production node server/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "next": "^10.0.3",
    "nodemon": "^2.0.6",
    "postcss": "^8.2.1",
    "replace-in-file": "^6.1.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-react-jsx-compat": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@react-spring/three": "^9.0.0-rc.3",
    "@react-three/drei": "^2.2.10",
    "@react-three/postprocessing": "^1.5.0",
    "babel-eslint": "^10.1.0",
    "babel-node": "^0.0.1-security",
    "babel-plugin-glsl": "^1.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "global": "^4.4.0",
    "glslify-loader": "^2.0.0",
    "husky": "^4.3.0",
    "lerp": "^1.0.3",
    "lint-staged": "^10.5.0",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.4.0",
    "next-images": "^1.6.0",
    "next-pwa": "^5.0.0",
    "next-react-svg": "^1.1.2",
    "next-transpile-modules": "^6.0.0",
    "next-videos": "^1.4.0",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react": "0.0.0-experimental-4ead6b530",
    "react-device-detect": "^1.14.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.0.0-rc.3",
    "react-three-fiber": "^5.3.10",
    "three": "^0.123.0",
    "use-dark-mode": "^2.3.1"
  },
  "license": "MIT"
}
